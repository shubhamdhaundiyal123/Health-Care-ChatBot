from tkinter import *

root = Tk()

root.geometry("500x500")
#To create frame
# store it in a variable, and pack it with an existing variable
f1 = Frame(root, bg="black", borderwidth=4)
#To print frame
f1.pack(side=LEFT, fill=Y)

f2 = Frame(root, bg="grey",borderwidth=5)
f2.pack(side=TOP,fill=X)

content1 = Label(f1, text="Hello",padx=100,pady=50)
content1.pack()
content2 = Label(f2, text="Welcome")
content2.pack()
root.mainloop()